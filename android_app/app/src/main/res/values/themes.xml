<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AceVentura56" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/yellow</item>
        <item name="colorPrimaryDark">@color/yellow_dark</item>
        <item name="colorPrimaryVariant">@color/yellow_dark</item>
        <item name="colorOnPrimary">@color/indigo</item>
        <!-- Secondary brand color. -->
<!--        <item name="colorSecondary">@color/yellow</item>
        <item name="colorSecondaryVariant">@color/yellow_dark</item>
        <item name="colorOnSecondary">@color/indigo</item>-->
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:fontFamily">@font/aileron_semibold</item>
        <item name="fontFamily">@font/aileron_semibold</item>
    </style>

    <style name="Theme.AceVentura56.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.AceVentura56.TranslucentStatus" parent="Theme.AceVentura56.NoActionBar">
        <item name="android:windowBackground">@color/yellow</item>
        <item name="android:colorBackground">@color/yellow</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>

    <style name="Theme.AceVentura56.Launcher">
        <item name="colorPrimary">@color/yellow</item>
        <item name="colorPrimaryDark">@color/yellow_dark</item>
        <item name="android:windowBackground">@drawable/launch_screen</item>
    </style>

    <style name="Theme.AceVentura56.AppBarOverlay" parent="ThemeOverlay.AppCompat.ActionBar" />

    <style name="Theme.AceVentura56.PopupOverlay" parent="ThemeOverlay.AppCompat.ActionBar" />

</resources>